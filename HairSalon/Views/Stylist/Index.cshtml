<div class='container'>
  <h1>Hair Sytlists</h1>
  <hr />

  <h2>Stylists</h2>
  @if (@Model.Count == 0)
  {
    <h4>No stylists have been added yet!</h4>
  }
  @foreach (var stylist in Model)
  {
    <h3><a href='/stylist/@stylist.GetId()'>@stylist.GetName()</a></h3>
  }
  <hr />
  <a href='/stylists/new'><button class='btn btn-warning'>Add Stylist</button></a>
  <a href='stylists/delete'><button>DELETE ALL</button></a>
  <a href='/'><button class='btn btn-success'>Back Home</button></a>
