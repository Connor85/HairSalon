<div class='container'>
    <h1>Specialty</h1>
    <hr />
    <ul>
        @foreach (var stylist in Model["specialty"])
        {
            <h4>@stylist.GetName()</h4>
        }
    </ul>
    <a href='/stylists/@Model["stylist"].GetId()/client/new'><button class='btn btn-danger'>Add Client</button></a>
    <a href='/specialties/'><button>Back to specialties</button></a>
    <a href='/stylists'><button class='btn btn-info'>Stylists</button></a>
</div>
